{% block body %}
    <h1>Liste des Étudiants</h1>



    <table style="border-collapse: collapse; width: 40%;">
        <thead>
            <tr>
                <th style="border: 1px solid #000;">ID</th>
                <th style="border: 1px solid #000;">Nom</th>
                <th style="border: 1px solid #000;">email</th>
                <th style="border: 1px solid #000;">Âge</th>
                 <th style="border: 1px solid #000;">Classroom</th>
                 <th style="border: 1px solid #000;">Action</th>
            

            </tr>
          
        </thead>
        <tbody>
            {% for t in response %}
            <tr>
                <td style="border: 1px solid #000;">{{ t.id }}</td>
                <td style="border: 1px solid #000;">{{ t.name }}</td>
                <td style="border: 1px solid #000;">{{ t.email }}</td>
                <td style="border: 1px solid #000;">{{ t.age }}</td>
                <td style="border: 1px solid #000;">{{ t.classroom }}</td>
                <td><button><a href="{{ path('removef', {'id': t.id}) }}" class="btn btn-primary">remove</a></button>
                  <button><a href="{{ path('update', {'id': t.id}) }}" class="btn btn-primary">update</a></button></td>
                 
            </tr>
            {% endfor %}
        </tbody>
        
    </table>
    
    <button><a href="{{ path('addf') }}" class="btn btn-primary">Add</a></button>
   
    
{% endblock %}



 

